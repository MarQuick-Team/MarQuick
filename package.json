{
	"name": "marquick",
	"displayName": "MarQuick",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:marquick.helloWorld",
		"onCommand:marquick.helloWorldPopup",
		"onCommand:marquick.helloWorldWrite"
	],
	"main": "./main.js",
	"contributes": {
		"commands": [
			{
				"command": "marquick.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "marquick.helloWorldPopup",
				"title": "Hello World Popup"
			},
			{
				"command": "marquick.helloWorldWrite",
				"title": "Hello World Write"
			}
		],
		"keybindings": [
			{
				"command": "marquick.helloWorldWrite",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"figlet": "^1.5.0",
		"vscode": "^1.1.37"
	}
}
